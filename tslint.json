{
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "semicolon": false,
        "quotemark": [true, "single", "jsx-double"],
        "trailing-comma": [
            true, {
                "multiline": "always",
                "singleline": "off"
            }
        ],
        "ordered-imports": false,
        "object-literal-sort-keys" : false
    }
}